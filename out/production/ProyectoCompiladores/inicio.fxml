                            <TableColumn fx:id="colFila" prefWidth="63.0" text="Fila" />
                            <TableColumn fx:id="colColumna" minWidth="0.0" prefWidth="78.0" text="Columna" />
                        </columns>
                        <columnResizePolicy>
                            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                        </columnResizePolicy>
                    </TableView>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
